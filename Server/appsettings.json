{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Configuration": {
    "CameraResolution": "1920x1080",
    "CameraFps": "30",
    "PhotoPath": "./photos",
    "PhotoTakeCronCycle": "0/10 * * ? * * *",
    "PhotoEnabled": "false",
    "PhotoDeleteCronCycle": "0 0 0 ? * 1/3 *",
    "PhotoDeleteOlderThan":  "2.00:00"
  }
}
