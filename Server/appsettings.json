{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Database": "Data Source=./data.db"
  },
  "AllowedHosts": "*",
  "Configuration": {
    "CameraResolution": "1280x720",
    "CameraFpsNumerator": "30",
    "CameraFpsDenominator": "1",
    "PhotoPath": "./photos",
    "PhotoTakeCronCycle": "0/10 * * ? * * *",
    "PhotoEnabled": "false",
    "PhotoDeleteCronCycle": "0 0 0 ? * 1/3 *",
    "PhotoDeleteOlderThan": "2.00:00",
    "DatabaseType": "sqlite",
    "UseVideo":  "false",
    "FFmpegPath":  "./ffmpeg"
  }
}