{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Configuration": {
    "CameraResolution": "1920x1080",
    "CameraFpsNumerator": "30",
    "CameraFpsDenominator": "1",
    "PhotoPath": "./photos",
    "PhotoTakeCronCycle": "0/10 * * ? * * *",
    "PhotoEnabled": "false",
    "PhotoDeleteCronCycle": "0 0 0 ? * 1/3 *",
    "PhotoDeleteOlderThan": "2.00:00"
  }
}
